{   
    "compilerOptions": {
        "target": "ESNext",
        "strict": true,
        "module": "esnext",
        "moduleResolution": "Node",
        "sourceMap": true,
        "outDir": "../testsOut",
        "resolveJsonModule": true,
        "esModuleInterop":true,
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": {
            "@index": [
                "src/index"
            ],
            "@pages/*": [
                "src/objects/*"
            ],
            "@fixtures/*": [
                "src/fixtures/*"
            ],
            "@addATeam/*": [
                "src/objects/addATeam/*"
            ],
            "@authPage/*": [
                "src/objects/authPage/*"
            ],
            "@createMeeting/*": [
                "src/objects/setupMeeting/*"
            ],
            "@inMeeting/*": [
                "src/objects/inMeeting/*"
            ],
            "@meetingsHomepage/*": [
                "src/objects/meetingsHomepage/*"
            ],
            "@myTasks/*": [
                "src/objects/myTasks/*"
            ],
            "@orgList/*": [
                "src/objects/orgList/*"
            ],
            "@settingsAndIntegrations/*": [
                "src/objects/settingsAndIntegrations/*"
            ],
            "@teamTasksDashboard/*": [
                "src/objects/teamTasksDashboard/*"
            ],
            "@timeline/*": [
                "src/objects/timeline/*"
            ],
            "@upgradeOrgPlan/*": [
                "src/objects/upgradeOrgPlan/*"
            ],
            "@userProfile/*": [
                "src/objects/userProfile/*"
            ], 
            "@web/*": [
                "src/screenplay/web/*"
            ],
            "@api/*": [
                "src/screenplay/api/*"
            ],
            "@screenplay/*": [
                "src/screenplay/*"
            ],
            "@general/*": [
                "src/objects/general/*"
            ],
            "@data/*": [
                "src/data/*"
            ]
        }
    }
}