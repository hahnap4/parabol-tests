{   
    "compilerOptions": {
        "target": "ESNext",
        "strict": true,
        "module": "commonjs",
        "moduleResolution": "Node",
        "sourceMap": true,
        "outDir": "../testsOut",
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "@index": [
                "src/index"
            ],
            "@pages/*": [
                "src/objects/*"
            ],
            "@fixtures/*": [
                "src/fixtures/*"
            ]
        }
    }
}